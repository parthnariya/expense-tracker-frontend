  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --ext ts,tsx --fix",
      "prettier --write"
    ],
    "*.{js,jsx,cjs,mjs}": [
      "eslint --ext ts,tsx --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
